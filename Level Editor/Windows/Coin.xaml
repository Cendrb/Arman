<Window x:Class="Level_Editor.Windows.Coin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        Title="Enter coin date" Height="145" Width="261" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Value" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,5,0,0"/>
        <xctk:IntegerUpDown x:Name="valueUpDown" FontSize="48" Margin="107,5,10,0" Value="10" DefaultValue="10" ToolTip="How much score will player get when collects it" Height="26" VerticalAlignment="Top"></xctk:IntegerUpDown>
        <Button Content="Create" Margin="10,0,0,10" Click="Button_Click" RenderTransformOrigin="-1.842,0.41" HorizontalAlignment="Left" Width="235" Height="28" VerticalAlignment="Bottom"/>
        <Label Content="Blast resistance" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,36,0,0"/>
        <xctk:DoubleUpDown x:Name="blastResistance" FontSize="48" Margin="107,36,10,0" Value="10" DefaultValue="10" ToolTip="How much score will player get when collects it" Height="26" VerticalAlignment="Top"/>
    </Grid>
</Window>
